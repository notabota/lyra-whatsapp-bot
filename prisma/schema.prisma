generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model chats {
  id   String  @id
  name String?
}

model messages {
  id       String  @id
  body     String?
  author   String?
  from     String?
  to       String?
  type     String?
  data     String?
  filename String?
  filesize Int?
  mimetype String?
}

model contacts {
  id        String  @id
  name      String?
  number    String?
  pushname  String?
  shortName String?
}
